[
    {
        "label": "About You",
        "input": "h1",
        "elements": [
            {
                "label": "Help us get to know you!",
                "input": "p"
            },
            {
                "label": "First Name",
                "caption": "Your (preferred) first name.",
                "input": "text",
                "key": "first_name",
                "required": true
            },
            {
                "label": "Surname",
                "caption": "The rest of your name, including middle name if you have one, and your last name.",
                "input": "text",
                "key": "surname"
            },
            {
                "label": "Major",
                "input": "dropdown",
                "other": true,
                "options": [
                    "Computer Science",
                    "Information Technology",
                    "Computer Engineering",
                    "Electrical Engineering",
                    "Digital Forensics",
                    "Cybersecurity and Privacy"
                ],
                "key": "major",
                "required": true
            },
            {
                "label": "GitHub Username",
                "caption": "Your GitHub username.",
                "input": "text",
                "key": "github",
                "required": true
            }
        ]
    },
    {
        "label": "Mentorship Interest",
        "input": "h1",
        "elements": [
            {
                "label": "What do you know, and want to know?",
                "input": "p"
            },
            {
                "label": "How long have you been in cybersecurity?",
                "caption": "This includes self-teaching and personal projects.",
                "input": "radio",
                "options": [
                    "I have absolutely no idea what I am doing.",
                    "Less than 3 months (but more than 0)",
                    "3 to 6 months",
                    "6+ months"
                ],
                "key": "mentee.time_in_cyber",
                "required": true
            },
            {
                "label": "What areas cybersecurity are you interested in?",
                "input": "checkbox",
                "options": [
                    "Pentesting, Vulnerability Assessments, and Red Teaming",
                    "Malware Development",
                    "Threat Intel",
                    "SOC/Active Defense",
                    "Digital Forensics/Incident Response",
                    "Malware Analysis/Reverse Engineering",
                    "Governance Risk and Compliance",
                    "Systems Administration",
                    "Challenge Creation",
                    "AppSec",
                    "Threat Hunting",
                    "Risk Management",
                    "idk what me want to do, pls help"
                ],
                "other": true,
                "required": true,
                "key": "mentee.domain_interest"
            },

            {
                "label": "Have you done any personal projects?",
                "caption": "If so, specify and include details. (what was the project? Why did you decide to? What did you learn from this project?)",
                "input": "text",
                "key": "mentee.personal_proj"
            },

            {
                "label": "What do you hope to gain from this mentorship program?",
                "input": "text",
                "key": "mentee.hope_to_gain",
                "required": true
            },

            {
                "input": "navigation",
                "next": "/join/signed",
                "next_label": "Submit",
                "prev": "/",
                "prev_label": "Cancel"
            }
        ]
    }
]
